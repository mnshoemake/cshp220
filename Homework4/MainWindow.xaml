<Window x:Class="Homework4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="20,20,0,0" TextWrapping="Wrap" Text="Zip Code" VerticalAlignment="Top"/>
        <TextBox x:Name="uxZipcode" TextChanged="UxZipcode_TextChanged" HorizontalAlignment="Left" Height="19" Margin="20,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="uxSubmit" Content="Submit" HorizontalAlignment="Left" Margin="20,75,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
